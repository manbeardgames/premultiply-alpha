    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>premultiplyalpha</AssemblyName>
    <Authors>Christopher Whitley</Authors>
    <Company>ManBeardGames</Company>
    <Description>CLI application used to premultiply the color data of image files.</Description>
